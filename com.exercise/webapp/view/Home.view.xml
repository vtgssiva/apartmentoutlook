<mvc:View
        controllerName="com.exercise.controller.Home"
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns:tnt="sap.tnt"
        xmlns:webc="sap.ui.webc.main"
        xmlns:core="sap.ui.core"
        xmlns:f="sap.ui.layout.form"
        xmlns:l="sap.ui.layout"
        height="100%">
    <tnt:ToolPage id="toolPage">
        <tnt:header>
            <tnt:ToolHeader>
                <Button
                        id="sideNavigationToggleButton"
                        icon="sap-icon://menu2"
                        type="Transparent"
                        press=".onSideNavButtonPress">
                    <layoutData>
                        <OverflowToolbarLayoutData priority="NeverOverflow"/>
                    </layoutData>
                </Button>
                <FlexBox direction="Row" alignItems="Start" class="sapUiTinyMarginBottom">
                    <tnt:InfoLabel id="il7" text="My Apartment" colorScheme="7"/>
                </FlexBox>
                <tnt:ToolHeaderUtilitySeparator/>
                <ToolbarSpacer>
                    <layoutData>
                        <OverflowToolbarLayoutData priority="NeverOverflow" minWidth="20px"/>
                    </layoutData>
                </ToolbarSpacer>
                <Button text="Admin" type="Transparent" press=".handleUserNamePress" ariaHasPopup="Menu">
                    <layoutData>
                        <OverflowToolbarLayoutData priority="NeverOverflow"/>
                    </layoutData>
                </Button>
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:sideContent>
            <tnt:SideNavigation
                    expanded="true"
                    selectedKey="{/selectedKey}"
                    itemSelect=".onItemSelect">
                <tnt:NavigationList items="{oLeftNavModel>/leftMenu}">
                    <tnt:NavigationListItem text="{oLeftNavModel>title}" key="{oLeftNavModel>reference}"/>
                </tnt:NavigationList>
            </tnt:SideNavigation>
        </tnt:sideContent>
        <tnt:mainContents>

            <NavContainer id="pageContainer1" initialPage="page1" visible="false">
                <pages>
                    <HBox items="{oHomeTileModel>/tileList}" wrap="Wrap">
                        <items>
                            <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop"
                                         header="{oHomeTileModel>header}" subheader="{oHomeTileModel>subheader}"
                                         press="press">
                                <TileContent unit="{oHomeTileModel>unit}" footer="{oHomeTileModel>footer}">
                                    <NumericContent value="{oHomeTileModel>value}" icon="{oHomeTileModel>icon}"
                                                    withMargin="false"/>
                                </TileContent>
                            </GenericTile>
                        </items>
                    </HBox>
                </pages>
            </NavContainer>

            <NavContainer id="pageContainer2" initialPage="page2" visible="false">
                <pages>
                    <VBox width="100%">

                        <VBox width="50%">
                            <FlexBox alignItems="Start"
                                     justifyContent="Center">
                                <Label text="Bill Number:"/>
                                <Input placeholder="Enter Bill Number"
                                       showSuggestion="true" class="sapUiMediumMarginBegin"/>
                            </FlexBox>
                            <FlexBox alignItems="Start"
                                     justifyContent="Center">
                                <Label text="Bill Date:"/>
                                <DatePicker placeholder="Enter Bill date"/>
                            </FlexBox>
                            <FlexBox alignItems="Start"
                                     justifyContent="Center">
                                <Label text="Service Provided:"/>
                                <ComboBox>
                                    <core:Item key="CAEB" text="Common Area Electricity Bill"/>
                                    <core:Item key="BA-CAEB" text="Block-A Common Area Electricity Bill"/>
                                    <core:Item key="BB-CAEB" text="Block-B Common Area Electricity Bill"/>
                                    <core:Item key="BC-CAEB" text="Block-C Common Area Electricity Bill"/>
                                </ComboBox>
                            </FlexBox>
                            <FlexBox alignItems="Start"
                                     justifyContent="Center">
                                <Button text="Submit" type="Accept"/>
                                <Button text="Cancel" type="Reject"/>
                            </FlexBox>
                        </VBox>

                        <VBox width="100%">
                            <webc:Table
                                    id="idProductsTable"
                                    stickyColumnHeader="true"
                                    rowClick="handleRowClick"
                                    mode="SingleSelect"
                                    rows="{oMaintenanceModel>/maintenance}">
                                <webc:columns>
                                    <webc:TableColumn>
                                        <Label text="Bill Date"/>
                                    </webc:TableColumn>
                                    <webc:TableColumn>
                                        <Label text="Bill No"/>
                                    </webc:TableColumn>
                                    <webc:TableColumn>
                                        <Label text="Service Provided"/>
                                    </webc:TableColumn>
                                    <webc:TableColumn>
                                        <Label text="Vendor"/>
                                    </webc:TableColumn>
                                    <webc:TableColumn>
                                        <Label text="Bill Amount"/>
                                    </webc:TableColumn>
                                    <webc:TableColumn>
                                        <Label text="Cleared Amount"/>
                                    </webc:TableColumn>
                                    <webc:TableColumn>
                                        <Label text="Outstanding Amount"/>
                                    </webc:TableColumn>
                                </webc:columns>
                                <webc:rows>
                                    <webc:TableRow type="Active">
                                        <webc:cells>
                                            <webc:TableCell>
                                                <webc:content>
                                                    <Label text="{oMaintenanceModel>bill_date}"/>
                                                </webc:content>
                                            </webc:TableCell>
                                            <webc:TableCell>
                                                <webc:content>
                                                    <Label text="{oMaintenanceModel>bill_no}"/>
                                                </webc:content>
                                            </webc:TableCell>
                                            <webc:TableCell>
                                                <webc:content>
                                                    <Label text="{oMaintenanceModel>service_provided}"/>
                                                </webc:content>
                                            </webc:TableCell>
                                            <webc:TableCell>
                                                <webc:content>
                                                    <Label text="{oMaintenanceModel>vendor}"/>
                                                </webc:content>
                                            </webc:TableCell>
                                            <webc:TableCell>
                                                <webc:content>
                                                    <Label text="{oMaintenanceModel>bill_amount}"/>
                                                </webc:content>
                                            </webc:TableCell>
                                            <webc:TableCell>
                                                <webc:content>
                                                    <Label text="{oMaintenanceModel>cleared_amount}"/>
                                                </webc:content>
                                            </webc:TableCell>
                                            <webc:TableCell>
                                                <webc:content>
                                                    <Label text="{oMaintenanceModel>outstanding_amount}"/>
                                                </webc:content>
                                            </webc:TableCell>
                                        </webc:cells>
                                    </webc:TableRow>
                                </webc:rows>
                            </webc:Table>
                        </VBox>
                    </VBox>
                </pages>
            </NavContainer>

            <NavContainer id="pageContainer3" initialPage="page3" visible="false">
                <pages>
                    <Page id="page">
                        <customHeader>
                            <Bar>
                                <contentRight>
                                    <Button id="edit" text="Edit" press="handleEditPress" />
                                    <Button id="save" text="Save" type="Emphasized" visible="false" press="handleSavePress" />
                                    <Button id="cancel" text="Cancel" visible="false" press="handleCancelPress" />
                                </contentRight>
                            </Bar>
                        </customHeader>
                        <content>
                            <VBox class="sapUiSmallMargin">
                                <f:SimpleForm id="SimpleFormChange354"
                                              editable="false"
                                              layout="ResponsiveGridLayout"
                                              title="Address"
                                              labelSpanXL="3"
                                              labelSpanL="3"
                                              labelSpanM="3"
                                              labelSpanS="12"
                                              adjustLabelSpan="false"
                                              emptySpanXL="4"
                                              emptySpanL="4"
                                              emptySpanM="4"
                                              emptySpanS="0"
                                              columnsXL="1"
                                              columnsL="1"
                                              columnsM="1"
                                              singleContainerFullSize="false" >
                                    <f:content>
                                        <Label text="Name" />
                                        <Input id="name" value="{SupplierName}" editable="false"/>
                                        <Label text="Street/No." />
                                        <Input id="street" value="{Street}" editable="false"/>
                                        <Input id="housenumber" value="{HouseNumber}" editable="false">
                                            <layoutData>
                                                <l:GridData span="XL1 L2 M2 S4" />
                                            </layoutData>
                                        </Input>
                                        <Label text="ZIP Code/City" />
                                        <Input id="zipcode" value="{ZIPCode}" editable="false">
                                            <layoutData>
                                                <l:GridData span="XL1 L2 M2 S4" />
                                            </layoutData>
                                        </Input>
                                        <Input id="city" value="{City}" editable="false"/>
                                        <Label text="Country" />
                                        <Select id="country" selectedKey="{Country}" editable="false">
                                            <items>
                                                <core:Item text="England" key="England"/>
                                                <core:Item text="Germany" key="Germany"/>
                                                <core:Item text="USA" key="USA"/>
                                            </items>
                                        </Select>
                                    </f:content>
                                </f:SimpleForm>
                            </VBox>
                        </content>
                    </Page>
                </pages>
            </NavContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
