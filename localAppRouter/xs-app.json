{
	"welcomeFile": "/apartmentoutlook/index.html#",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [
		{
			"source": "^/apartmentoutlook/(.*)$",
			"target": "/$1",
			"localDir": "../com.exercise/webapp/"
		},
		{
			"source": "^/ui5/(.*)$",
			"target": "/$1",
			"localDir": "../ui5/"
		}
	]
}